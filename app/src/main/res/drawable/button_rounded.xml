<?xml version="1.0" encoding="utf-8"?>
<!--suppress AndroidElementNotAllowed -->
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <padding
        android:bottom="7dp"
        android:left="7dp"
        android:right="7dp"
        android:top="7dp" tools:ignore="StateListReachable" />

    <item
        android:state_pressed="true"
        android:drawable="@drawable/button_rounded_press" tools:ignore="StateListReachable" />

    <item
        android:state_pressed="false"
        android:drawable="@drawable/button_rounded_unpress" />

</selector>